extends ../layouts/default

block variables
	- const title = "TIQUM";

block content
	.wrapper.main-page
		include ../../blocks/modules/header/header
		.page(style="position:relative" )
			.new-page.new-page1
				.new-page__image(data-depth='0.2')
					img(src="img/pages/new-page/img1.jpg", alt="alt")
				.new-page__header 
					.container
						.flex-lr 
							.flex-lr__left 
							.flex-lr__right 
								.new-page__title-small Консалтинг по цифровой<br> трансформации
				.new-page1__serveces-title 
					.container
						.flex-lr 
							.flex-lr__left 
								p Другие услуги
							.flex-lr__right 
				.new-page1__services
					.container
						.flex-lr 
							.flex-lr__left 
								ul
									li 
										a(href="#") Продуктовая веб- и мобильная разработка
									li 
										a(href="#") Продуктовая веб- и мобильная разработка
									li 
										a(href="#") Дизайн и проектирование интерфейсов
									li 
										a(href="#") Развитие и поддержка IT-проектов
									li 
										a(href="#") MVP за 3 месяца
									li 
										a(href="#") Метод IQ
							.flex-lr__right 
								.new-page1__services-items 
									.item-services-new-page1
										.item-services-new-page1__title.general-title Проводим сессии дизайн-<br>мышления 
											span и IT-аудит,<br> формируем стратегию<br> цифровой трансформации<br> для решения бизнес-задач
										.item-services-new-page1__number 01
									.item-services-new-page1
										.item-services-new-page1__title.general-title Улучшаем бизнес-<br>показатели 
											span и ускоряем<br> работу интернет-проектов
										.item-services-new-page1__number 02
									.item-services-new-page1
										.item-services-new-page1__title.general-title UX-аудит: улучшаем<br> конверсию
										.item-services-new-page1__number 03
									.item-services-new-page1
										.item-services-new-page1__title.general-title Строим IT-архитектуру<br> под цели компании
										.item-services-new-page1__number 04
									.item-services-new-page1.item-services-new-page1--small
										p Используем свой опыт в digital и знания, накопленные за 15 лет, для формирования стратегии цифровой трансформации бизнеса

		include ../../blocks/modules/footer/footer

block scripts
	script(src="https://code.jquery.com/jquery-3.6.1.min.js") 
	script(src="https://cdn.jsdelivr.net/npm/swiper/swiper-bundle.min.js")
	script(src="js/main.js")
